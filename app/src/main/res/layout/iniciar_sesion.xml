<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="¿Olvido su contraseña?"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="751dp" />

    <View
        android:id="@+id/view"
        android:layout_width="412dp"
        android:layout_height="708dp"
        android:background="#EC7484"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="-4dp" />

    <View
        android:id="@+id/view4"
        android:layout_width="356dp"
        android:layout_height="666dp"
        android:background="@drawable/view_redondo"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="107dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="33dp"
        android:layout_height="23dp"
        app:srcCompat="@drawable/vector__7_"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="214dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Iniciar sesión:"
        android:textColor="#FFFDFD"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="40dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="205dp"
        android:layout_height="227dp"
        app:srcCompat="@drawable/img_mastografias1"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="462dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="262dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Email:"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="148dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="305dp"
        android:layout_height="45dp"
        android:text="Guardar Contraseña"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="270dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="262dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Contraseña:"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="207dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="222dp"
        android:layout_height="46dp"
        android:background="@drawable/button_redondo"
        android:text="Iniciar Sesión"
        app:backgroundTint="#EC7484"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="376dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="¿Olvido su contraseña?"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="751dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Soy Nuevo Usuario: Registrate"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="797dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="29dp"
        android:layout_height="19dp"
        app:srcCompat="@drawable/vector__6_"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="162dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
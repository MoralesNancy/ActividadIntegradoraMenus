<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="¿Olvido su contraseña?"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="751dp" />

    <View
        android:id="@+id/view"
        android:layout_width="412dp"
        android:layout_height="708dp"
        android:background="#EC7484"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="-4dp" />

    <View
        android:id="@+id/view4"
        android:layout_width="356dp"
        android:layout_height="666dp"
        android:background="@drawable/view_redondo"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="107dp" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="33dp"
        android:layout_height="23dp"
        app:srcCompat="@drawable/vector__10_"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="250dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="¿Olvidaste tu contraseña?"
        android:textColor="#FFFDFD"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="39dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="205dp"
        android:layout_height="227dp"
        app:srcCompat="@drawable/mastografias_1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="503dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="264dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Email:"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="241dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="222dp"
        android:layout_height="46dp"
        android:background="@drawable/button_redondo"
        android:text="Enviar"
        app:backgroundTint="#EC7484"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="358dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancelar"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="751dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ya tengo una cuenta: Iniciar Sesion"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="797dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="317dp"
        android:layout_height="72dp"
        android:text="Ingresa tu correo electronico / para enviar el código de seguridad"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="148dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
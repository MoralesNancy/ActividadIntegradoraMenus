<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="¿Olvido su contraseña?"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="751dp" />

    <View
        android:id="@+id/view"
        android:layout_width="412dp"
        android:layout_height="708dp"
        android:background="#EC7484"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="-4dp" />

    <View
        android:id="@+id/view4"
        android:layout_width="356dp"
        android:layout_height="666dp"
        android:background="@drawable/view_redondo"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="107dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Establecer nueva contraseña"
        android:textColor="#FFFDFD"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="41dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="205dp"
        android:layout_height="227dp"
        app:srcCompat="@drawable/mastografias_1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="511dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="222dp"
        android:layout_height="46dp"
        android:background="@drawable/button_redondo"
        android:text="Finalizar"
        app:backgroundTint="#EC7484"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancelar"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="751dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ya tengo una cuenta: Iniciar Sesion"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="797dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="317dp"
        android:layout_height="72dp"
        android:text="Para acceder / proteger tu cuenta, crea una nueva contraseña."
        android:textSize="16sp"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="148dp" />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="310dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Código:"
        android:inputType="number"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="221dp" />

    <EditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="310dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Nueva contraseña:"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="282dp" />

    <EditText
        android:id="@+id/editTextTextPassword4"
        android:layout_width="311dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Confirmar contraseña:"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="348dp" />

</androidx.constraintlayout.widget.ConstraintLayout>